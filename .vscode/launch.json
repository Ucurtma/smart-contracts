  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Migrations",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/.bin/truffle",
      "args": [
        "migrate"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Merger",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/.bin/sol-merger",
      "args": [
        // "./contracts/ERC20FundingContract.sol",
        // "./contracts/BiliraFundingContractDeployer.sol",
        // "./contracts/DeploymentManager.sol",
        "./contracts/FundingManagerContract.sol",
        "./build"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Deploy new Funding",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/scripts/deploy-contracts.js",
      "args": [
        "-p", 
        "10",
        "-w", 
        "28", 
        "-e", 
        "60",
        "-t", 
        "0x31f1cfbbbd9ce6ee0f8d9c79828b0b099653daa0", 
        "-o", 
        "0xFb4BA4f9a0d80ffda8b11bbcDCD56a0eDd7729E2" ,
        "--rinkeby"
      ]
    }
  ]
}